{% extends "base.djhtml" %}
{% block title %} Add A Folder {% endblock %}
{% block content %}
    <h1>Add A Folder</h1>
    <form id="add_folder_form" method="post" action="/folders/add/">
      {% csrf_token %}
      Folder Name: <input class="form-control" type="text" name="name" value="" size="50"/>
      <br/>
      <input class="form-control"  type="submit" value="submit"/>
    </form>	
{% endblock %}
